    <%= form_with model:  @parent_square, url: parent_square_path(@parent_square.id) do |f| %>
    <div class="squares_wrapper">
      <div class="square_wrapper">
        <div class="d-flex justify-content-center mt-5">
          <div class=""><%= f.text_area :row_1, class: 'form-control', size: "5x3" %></div>
          <div class=""><%= f.text_area :row_2, class: 'form-control', size: "5x3" %></div>
          <div class=""><%= f.text_area :row_3, class: 'form-control', size: "5x3" %></div>
        </div>
        <div class="d-flex justify-content-center">
          <div class=""><%= f.text_area :row_4, class: 'form-control', size: "5x3" %></div>
          <textarea rows="3" cols="7" disabled><%= @parent_square.row_5 %></textarea><!--真ん中のマス-->
         <div class=""><%= f.text_area :row_6, class: 'form-control', size: "5x3" %></div>
        </div>
        <div class="d-flex justify-content-center">
          <div class=""><%= f.text_area :row_7, class: 'form-control', size: "5x3" %></div>
          <div class=""><%= f.text_area :row_8, class: 'form-control', size: "5x3" %></div>
          <div class=""><%= f.text_area :row_9, class: 'form-control', size: "5x3" %></div>
        </div>
      </div>
      <%= f.fields_for :child_squares do |child_square_fields| %>
        <div class="square_wrapper">
          <div class="d-flex justify-content-center mt-5">
            <div class=""><%= child_square_fields.text_area :row_1, class: 'form-control', size: "5x3" %></div>
            <div class=""><%= child_square_fields.text_area :row_2, class: 'form-control', size: "5x3" %></div>
            <div class=""><%= child_square_fields.text_area :row_3, class: 'form-control', size: "5x3" %></div>
          </div>
          <div class="d-flex justify-content-center">
            <div class=""><%= child_square_fields.text_area :row_4, class: 'form-control', size: "5x3" %></div>
            <textarea rows="3" cols="7" disabled><%= child_square_fields.object.row_5 %></textarea><!--真ん中のマス-->
            <div class=""><%= child_square_fields.text_area :row_6, class: 'form-control', size: "5x3" %></div>
          </div>
          <div class="d-flex justify-content-center">
            <div class=""><%= child_square_fields.text_area :row_7, class: 'form-control', size: "5x3" %></div>
            <div class=""><%= child_square_fields.text_area :row_8, class: 'form-control', size: "5x3" %></div>
            <div class=""><%= child_square_fields.text_area :row_9, class: 'form-control', size: "5x3" %></div>
          </div>
        </div>
      <% end %>
      </div>
      <div><%= f.submit '保存' %></div>
    <% end %>
