<div class="container-fluid registration py-5">
  <div class="row">
    <div class="offset-1">
    <h2>アイデア一覧</h2>
  </div>
</div>
<table class="table">
  <thead>
    <tr>
      <th>メインテーマ</th>
      <th>作成日時</th>
      <th>
        <%= form_with model: @parent_square do |f| %>
        新しいアイデアを作成する<%= f.text_field :row_5 %>
        <span><%= f.submit '作成' %></span>
        <% end %>
      </th>
    </tr>
  </thead>
  <tbody>
    <% @parent_squares.each do |parent_squares| %>
    <tr>
      <th scope="row"><%= link_to parent_squares.row_5, parent_square_path(parent_squares.id) %></th>
      <td><%= parent_squares.created_at.strftime('%Y/%m/%d') %></td>
    </tr>
    <% end %>
  </tbody>
</table>